{
  "version": 2,
  "builds": [
    { "src": "api/api.py", "use": "@vercel/python" },
    {
      "src": "my-app/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/api.py" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
